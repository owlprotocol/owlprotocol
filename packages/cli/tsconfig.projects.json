{
  "compilerOptions": {
    "target": "es2020",
    "module": "es2020",
    "moduleResolution": "node",
    "sourceMap": false,
    "strict": true,
    "esModuleInterop": false,
    "resolveJsonModule": true,
    "declaration": true,
    "emitDeclarationOnly": false,
    "skipLibCheck": true,
    "downlevelIteration": true,
    "isolatedModules": true,
    "types": ["node"],
    "allowSyntheticDefaultImports": true
  },
  "include": ["projects"]
}
