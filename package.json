{
  "name": "owlprotocol",
  "version": "1.0.0",
  "description": "Owl Protocol Public Repo",
  "scripts": {
    "build": "turbo run build --continue",
    "build:contracts": "turbo run build --filter @owlprotocol/contracts",
    "build:web3-redux": "turbo run build --filter @owlprotocol/web3-redux",
    "build:web3-redux-components": "turbo run build --filter @owlprotocol/web3-redux-components",
    "build-storybook:web3-redux-components": "turbo run build-storybook --filter @owlprotocol/web3-redux-components",
    "clean:node_modules": "rimraf node_modules ./**/node_modules"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "type": "module",
  "packageManager": "pnpm@7.15.0",
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@owlprotocol/eslint-config": "workspace:*",
    "cross-env": "^7.0.3",
    "husky": "^7.0.4",
    "node-gyp": "^9.3.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "turbo": "^1.8.3"
  }
}
